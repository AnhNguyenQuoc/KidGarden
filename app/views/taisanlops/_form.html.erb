<%= form_with(model: taisanlop, local: true) do |form| %>
  <% if taisanlop.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(taisanlop.errors.count, "error") %> prohibited this taisanlop from being saved:</h2>

      <ul>
      <% taisanlop.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :tentaisan %>
    <%= form.text_field :tentaisan, id: :taisanlop_tentaisan %>
  </div>

  <div class="field">
    <%= form.label :donvitaisan %>
    <%= form.text_field :donvitaisan, id: :taisanlop_donvitaisan %>
  </div>

  <div class="field">
    <%= form.label :nguontaisan %>
    <%= form.text_field :nguontaisan, id: :taisanlop_nguontaisan %>
  </div>

  <div class="field">
    <%= form.label :soluong %>
    <%= form.number_field :soluong, id: :taisanlop_soluong %>
  </div>

  <div class="field">
    <%= form.label :lop_id %>
    <%= form.collection_select :lop_id, Lop.all, :id, :lophoc, id: :taisanlop_lop_id, :prompt => "Chon lop" %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
